{
    "workbench.colorTheme": "Default Dark+",
    "cmake.configureOnOpen": true,
    // "window.zoomLevel": 0.8,
    "editor.minimap.enabled": true,
    "C_Cpp.autocomplete": "Default",
    "[cpp]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "[c]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "files.associations": {
        "in.h": "c",
        "types.h": "c",
        "socket.h": "c",
        "string.h": "c",
        "stdlib.h": "c"
    }
}
